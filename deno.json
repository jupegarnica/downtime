{
  "tasks": {
    "dev": "deno run --watch -A  cli.ts",
    "test": "deno test -A",
    "cli": "deno run -A cli.ts",
    "install": "deno install  -rgA -fn downtime cli.ts",
    "install:x": "deno install -rgA -fn downtime https://deno.land/x/downtime/cli.ts",
    "install:jsr": "deno install -rgA -fn downtime jsr:@garn/downtime",
    "server": "deno run -A server.ts",
    "version":"deno run -A jsr:@utility/version patch",
    "release": "deno task version && git push && git push --tags && deno publish"
  },
  "lock": false,
  "name": "@garn/downtime",
  "exports": "./cli.ts",
  "version": "1.0.17"
}
