{
  "tasks": {
    "dev": "deno run --watch -A  cli.ts",
    "test": "deno test --A",
    "cli": "deno run -A cli.ts",
    "install": "deno install --allow-read --allow-net --allow-env -fn downtime cli.ts",
    "install:x": "deno install --A -fn downtime https://deno.land/x/downtime/cli.ts",
    "server": "deno run -A server.ts",
    "release": "deno run  -A https://deno.land/x/version/index.ts patch && git push && git push --tags"
  },
  "lock": false
}
